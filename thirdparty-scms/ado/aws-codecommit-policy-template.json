{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "codecommit:CreateBranch",
                "codecommit:ListBranches",
                "codecommit:CreateRepository",
                "codecommit:CreateCommit",
                "codecommit:GitPush"
            ],
            "Resource": "arn:aws:codecommit:REGION:ACCOUNT_ID:sdlf-*"
        },
        {
            "Effect": "Allow",
            "Action": "codecommit:ListRepositories",
            "Resource": "*"
        }
    ]
}